add wave -noupdate /testbench_floatingpointconvertor/B
add wave -noupdate -radix fpoint#23 /testbench_floatingpointconvertor/Out1
add wave -noupdate -radix fpoint#23 /testbench_floatingpointconvertor/Out2
add wave -noupdate -radix fpoint#23 /testbench_floatingpointconvertor/expected1
add wave -noupdate -radix fpoint#23 /testbench_floatingpointconvertor/expected2
TreeUpdate [SetDefaultTree]
WaveRestoreCursors {{Cursor 1} {168782 ps} 0}
quietly wave cursor active 1
configure wave -namecolwidth 298
